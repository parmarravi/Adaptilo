<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/primary" />
            <size
                android:width="60dp"
                android:height="60dp" />
        </shape>
    </item>

    <item
        android:bottom="7dp"
        android:left="7dp"
        android:right="7dp"
        android:top="7dp">
        <shape android:shape="oval">
            <solid android:color="@color/primary_dark" />
            <size
                android:width="60dp"
                android:height="60dp" />
        </shape>
    </item>

</layer-list>